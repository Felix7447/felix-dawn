<div class="logo-list-section">
  <div class="logo-list-container page-width">
    {% for item in section.blocks %}{% endfor %}
    <div class="logo-list-item">a</div>

  </div>
</div>

{% schema %}
  {
    "name": "logo-list-section",
    "blocks": [
      {
        "name": "logo-list-item",
        "type": "block",
        "settings": [
          {
            "type": "image_picker",
            "id": "logo",
            "label": "Logo"
          }, {
            "type": "text",
            "id": "title",
            "label": "Title"
          }, {
            "type": "text",
            "id": "text",
            "label": "Text"
          }, {
            "type": "url",
            "id": "link",
            "label": "Link"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Logo List section"
      }
    ]
  }
{% endschema %}